{
  "name": "partition-size",
  "version": "0.0.7",
  "description": "Splits an array by size",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/partition-size.git"
  },
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "start": "coffee -cw -o lib/ src/",
    "test": "mocha --require espower-coffee/guess test/**/*.coffee",
    "lint": "coffeelint -f coffeelint.json src/",
    "compile": "coffee -c -o lib/ src/",
    "standalone": "browserify src/partition-size.coffee -t coffeeify -s partitionSize | derequire > build/partition-size.js",
    "minify": "uglifyjs build/partition-size.js -m -c warnings=false --comments '/\\/*!/' -o build/partition-size.min.js",
    "build": "npm ru lint && npm ru compile && npm ru standalone && npm ru minify"
  },
  "engines": {
    "node": "0.12.x"
  },
  "author": "sugarshin",
  "license": "MIT",
  "dependencies": {
    "lodash.partition": "^3.1.1"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "coffeelint": "^1.10.1",
    "derequire": "^2.0.0",
    "espower-coffee": "^0.10.0",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "uglify-js": "^2.4.23"
  }
}
