{
  "name": "partition-size",
  "version": "1.0.0",
  "description": "Splits an array by size",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/partition-size.git"
  },
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src/",
    "test": "npm run lint && mocha --compilers js:espower-babel/guess test/**/*.js",
    "compile": "babel -d lib/ src/",
    "standalone": "browserify src/partition-size.js -t babelify -s partitionSize | derequire > build/partition-size.js",
    "minify": "uglifyjs build/partition-size.js -m -c warnings=false --comments '/\\/*!/' -o build/partition-size.min.js",
    "build": "npm run lint && npm run compile && npm run standalone && npm run minify"
  },
  "engines": {
    "node": "0.12.x"
  },
  "author": "sugarshin",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babelify": "^6.2.0",
    "browserify": "^11.0.1",
    "derequire": "^2.0.2",
    "eslint": "^1.3.0",
    "espower-babel": "^3.3.0",
    "mocha": "^2.2.5",
    "power-assert": "^1.0.0",
    "uglify-js": "^2.4.24"
  }
}
